#include <iostream>
#include <cmath>

using namespace std;
int main(int argc, char *argv[])
{
  // Declare any other variables you need here
  // and/or delete the integer declared below 
  double tempF, tempC;
  
  // Prompt the user
  cout << "Enter a number:  ";

  // get input  
  cin >> tempF;
cout << endl;

  // Do some operations
tempC = (tempF - 32.0)*5.0/9;


  // Print the result to the user
cout << "The temperature in Celcius is " << tempC << endl;
  
  // You're done
  return 0;
  
}

